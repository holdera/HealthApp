<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="health\userBundle\Entity\UsersInfo" table="users_Info">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="firstname" type="string" column="firstname" length="80" nullable="false"/>
    <field name="lastname" type="string" column="lastname" length="80" nullable="false"/>
    <field name="email" type="string" column="email" length="80" nullable="false"/>
    <field name="password" type="string" column="password" length="80" nullable="false"/>
    <field name="sQuestion" type="string" column="s_question" length="255" nullable="false"/>
    <field name="sAnswer" type="string" column="s_answer" length="80" nullable="false"/>
  </entity>
</doctrine-mapping>
