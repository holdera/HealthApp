<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="health\userBundle\Entity\UserProfile" table="user_profile">
    <id name="id" type="integer" column="Id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="firstname" type="string" column="firstname" length="30" nullable="false"/>
    <field name="lastname" type="string" column="lastname" length="30" nullable="false"/>
    <field name="gender" type="string" column="gender" length="11" nullable="false"/>
    <field name="dob" type="date" column="dob" nullable="false"/>
    <field name="email" type="string" column="email" length="50" nullable="false"/>
    <field name="cnumber" type="string" column="cnumber" length="10" nullable="false"/>
    <field name="city" type="string" column="city" length="20" nullable="false"/>
    <field name="province" type="string" column="province" length="20" nullable="false"/>
    <field name="country" type="string" column="country" length="20" nullable="false"/>
    <field name="image" type="string" column="image" length="30" nullable="false"/>
    <field name="height" type="string" column="height" length="4" nullable="false"/>
    <field name="weight" type="string" column="weight" length="4" nullable="false"/>
    <field name="bodyType" type="string" column="body_type" length="11" nullable="false"/>
    <field name="dweight" type="string" column="dweight" length="4" nullable="false"/>
    <field name="hproblem" type="text" column="hproblem" nullable="false"/>
    <field name="alevel" type="string" column="alevel" length="11" nullable="false"/>
  </entity>
</doctrine-mapping>
