<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="health\healthUserBundle\Entity\Words" table="words">
    <unique-constraints>
      <unique-constraint name="lang_id" columns="lang_id"/>
    </unique-constraints>
    <id name="wordId" type="boolean" column="word_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="langId" type="boolean" column="lang_id" nullable="false"/>
    <field name="title" type="string" column="title" length="80" nullable="false"/>
    <field name="intro" type="text" column="intro" nullable="false"/>
    <field name="home" type="string" column="home" length="30" nullable="false"/>
    <field name="forumHome" type="string" column="forum_home" length="40" nullable="false"/>
    <field name="language" type="string" column="language" length="40" nullable="false"/>
    <field name="register" type="string" column="register" length="30" nullable="false"/>
    <field name="login" type="string" column="login" length="30" nullable="false"/>
    <field name="logout" type="string" column="logout" length="30" nullable="false"/>
    <field name="newThread" type="string" column="new_thread" length="40" nullable="false"/>
    <field name="subject" type="string" column="subject" length="30" nullable="false"/>
    <field name="body" type="string" column="body" length="30" nullable="false"/>
    <field name="submit" type="string" column="submit" length="30" nullable="false"/>
    <field name="postedOn" type="string" column="posted_on" length="30" nullable="false"/>
    <field name="postedBy" type="string" column="posted_by" length="30" nullable="false"/>
    <field name="replies" type="string" column="replies" length="30" nullable="false"/>
    <field name="latestReply" type="string" column="latest_reply" length="40" nullable="false"/>
    <field name="postAReply" type="string" column="post_a_reply" length="40" nullable="false"/>
  </entity>
</doctrine-mapping>
