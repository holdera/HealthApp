<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="health\healthUserBundle\Entity\Languages" table="languages">
    <unique-constraints>
      <unique-constraint name="lang" columns="lang"/>
    </unique-constraints>
    <id name="langId" type="boolean" column="lang_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="lang" type="string" column="lang" length="60" nullable="false"/>
    <field name="langEng" type="string" column="lang_eng" length="20" nullable="false"/>
  </entity>
</doctrine-mapping>
