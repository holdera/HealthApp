<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="health\ResourcesBundle\Entity\Forum" table="forum">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="30" nullable="false"/>
    <field name="content" type="text" column="content" nullable="false"/>
    <field name="permalinkParent" type="string" column="permalink_parent" length="130" nullable="false"/>
    <field name="author" type="string" column="author" length="100" nullable="false"/>
    <field name="permalink" type="string" column="permalink" length="130" nullable="false"/>
    <field name="date" type="datetime" column="date" nullable="false"/>
  </entity>
</doctrine-mapping>
